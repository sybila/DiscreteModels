<MODEL ver="1.0">
	<STRUCTURE>
		<SPECIE name="A">
			<REGUL source="A" />	
			<REGUL source="B" />
			<LOGIC formula="(A|!B)" />
		</SPECIE>
		<SPECIE name="B">
			<REGUL source="A" />	
			<REGUL source="B" />
			<LOGIC formula="A" />			
		</SPECIE>
	</STRUCTURE>
	<SERIES>
		<EXPR values="(A=0&B=0)" />
		<EXPR values="(A=1&B=1)" />
	</SERIES>
</MODEL>
