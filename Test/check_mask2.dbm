<MODEL ver="1.1">
	<STRUCTURE>
		<SPECIE name="cI" max="2" >
			<REGUL source="cI" threshold ="2" label="mon+" />
			<REGUL source="cII" threshold ="1" label="+" />
			<REGUL source="cro" threshold ="1" label="-" />		
		</SPECIE>
		<SPECIE name="cII" max="1" >
			<REGUL source="cI" threshold ="2" label="-" />
			<REGUL source="cro" threshold ="3" label="-" />
			<REGUL source="N" threshold ="1" label="+" />			
		</SPECIE>
		<SPECIE name="cro" max="3" >
			<REGUL source="cI" threshold ="2" label="-" />
			<REGUL source="cro" threshold ="3" label="-" />		
		</SPECIE>
		<SPECIE name="N" max="1" >
			<REGUL source="cI" threshold ="1" label="-" />
			<REGUL source="cro" threshold ="2" label="-" />		
		</SPECIE>
	</STRUCTURE>
	<SERIES>
		<EXPR values="(((cI=0&amp;cII=0)&amp;cro=0)&amp;N=0)" />
		<EXPR values="(((cI=0&amp;cII=0)&amp;cro=2)&amp;N=1)" />
		<EXPR values="(((cI=0&amp;cII=0)&amp;cro=2)&amp;N=0)" />
		<EXPR values="(((cI=0&amp;cII=0)&amp;cro=3)&amp;N=0)" />
		<EXPR values="(((cI=0&amp;cII=0)&amp;cro=2)&amp;N=0)" />
	</SERIES>
</MODEL>

